<?xml version="1.0"?>
<robot name="aidu" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="base_link" />
  
  <link name="main_link">
    <visual>
      <geometry>
        <box size="0.57 0.46 0.08"/>
      </geometry>
    </visual>
  </link>

  <link name="camera_link">
  </link>
  
